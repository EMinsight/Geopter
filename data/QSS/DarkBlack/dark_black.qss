/* ------------------------------- QWIIDGETS --------------------------------------------*/

QLabel:enabled {
    color: rgba(208,208,208,1);
    background: rgba(0, 0, 0, 0);
}

QLabel:disabled {
    color: rgba(136,136,136,1);
    background: rgba(0, 0, 0, 0);
}

QPushButton:enabled {
    background: rgba(85,85,85,1);
    border: 1px solid rgba(39,46,56,1);
    selection-background-color: rgba(176,210,241,1);  
    border-radius: 2px;
    height: 24px;
    padding-left: 10px;
    padding-right: 10px;
}

QPushButton:disabled {
    color: rgba(136,136,136,1);
    background: transparent;
    border: 1px solid rgba(170,170,170,1);
    border-radius: 2px;
    height: 24px;
    padding-left: 10px;
    padding-right: 10px;
}

QPushButton:hover{
    border: 1px solid rgba(20,140,210,1);
    border-radius: 2px;
    padding-left: 10px;
    padding-right: 10px;
}

QPushButton:checked{
    border: 1px solid transparent;
    background-color:rgba(30,30,30,1);
    border-right:1px solid rgba(20,20,20,1);
    border-bottom:1px solid rgba(20,20,20,1);
}

QLineEdit:enabled {
    color: rgba(255,255,255,1);
    background: rgba(40,40,40,1);
    border: 1px solid rgba(150,150,150,1);
    border-radius:0px;
    height:19px;
}

QLineEdit:disabled {
    color: rgba(77,77,77,1);
    background: rgba(40,40,40,1);
    border 1px: solid rgba(150,150,150,1):
    height:19px;
}

QCheckBox:enabled {
    color: rgba(208,208,208,1);
    background: transparent;
}

QCheckBox:disabled {
    color: rgba(136,136,136,1);
    background: transparent;
}

QCheckBox::indicator {
    width: 10px;
    height: 10px;
    border: 1px solid rgba(136,136,136,1);
    border-radius: 2px;
}

QCheckBox::indicator::checked {
    image: url(:/icons/widgets/checkbox_checked_16.png);
    border: 1px solid rgba(28,131,255,1);
}

QCheckBox::indicator::checked:disabled {
    image: url(:/icons/widgets/checkbox_disabled_16.png);
    border: 1px solid rgba(0,0,0,1);
}

QCheckBox::indicator::unchecked {
    background: rgba(208,208,208,1);
}

QCheckBox::indicator::unchecked:disabled {
    background: rgba(40,40,40,1);
    border: 1px solid rgba(0,0,0,1);
}

OverlaySlider {
    background: transparent;
}


QSlider::groove:horizontal {
    border: none;
    height: 4px; /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
    background-color: rgba(52,52,52,1);
    margin: 2px 0;
}

QSlider::handle:horizontal {
    background-color: rgba(40,40,40,0.85);
    border: 1px solid rgba(85,85,85,1);
    width: 10px;
    margin: -8px 0; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
    border-radius: 2px;
}

QSlider::handle:horizontal:hover {
    border: 1px solid rgba(28,131,255,0.7);
}

QSlider::handle:horizontal:pressed {
    background: 1px solid rgba(28,131,255,0.3);
}

QSlider::sub-page:horizontal {
    border: none;
    height: 3px; /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
    background: rgba(28,131,255,0.7);
    margin: 2px 0;
}

/* ------------------------------- TREE VIEW --------------------------------------------*/

QTreeView {
    background-color: rgba(30,30,30,1);
    alternate-background-color: rgba(35,35,35,1);
    color:rgba(208,208,208,1);
}


QHeaderView {
    background-color:  rgba(30,30,30,1);
    border-top: 1px solid rgba(68,68,68,1);
    border-bottom: 1px solid rgba(68,68,68,1);
    border-left: 1px solid rgba(68,68,68,1);
    font-size: 12px;
    padding-top: 0px;
    padding-bottom: 0px;
    height: 20px;
}

/*SLIDER SECTIONS */
QHeaderView::section {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                      stop:0 rgba(30,30,30,1), stop: 0.5 rgba(30,30,30,1),
                                      stop: 0.6 rgba(30,30,30,1), stop:1 rgba(30,30,30,1));
    border:none;
    padding-left:3px;
    border-right: 1px solid rgba(68,68,68,1);
}


QTreeView::branch:has-siblings:!adjoins-item {
    border-image: url(:/icons/widgets/dark/branch_vline.png) 0;
}

QTreeView::branch:has-siblings:adjoins-item {
    border-image: url(:/icons/widgets/dark/branch_more.png) 0;
}

QTreeView::branch:!has-children:!has-siblings:adjoins-item {
    border-image: url(:/icons/widgets/dark/branch_end.png) 0;
}

QTreeView::branch:has-children:!has-siblings:closed{
        border-image: none;
        image: url(:/icons/widgets/dark/branch_minus_arrow_end.png);
}

QTreeView::branch:closed:has-children:has-siblings {
        border-image: none;
        image: url(:/icons/widgets/dark/branch_minus_arrow_inter.png);
}

QTreeView::branch:open:has-children:!has-siblings {
        border-image: none;
        image: url(:/icons/widgets/dark/branch_plus_arrow_end.png);
}

QTreeView::branch:open:has-children:has-siblings  {
        border-image: none;
        image: url(:/icons/widgets/dark/branch_plus_arrow_inter.png);
}

