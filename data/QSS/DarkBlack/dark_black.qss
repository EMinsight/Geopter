    color: rgba(208,208,208,1);
    background-color: rgba(0, 0, 0, 0);
    font-size: 7pt;
}

QWidget#DelegateHeader:disabled {
    color: rgba(208,208,208,0.6);
    background-color: rgba(0, 0, 0, 0);
    font-size: 7pt;
}
QWidget#DelegateHeaderHighlite:enabled {
    color: rgba(208,208,208,1);
    background-color: rgba(52,52,52,1);
    font-size: 7pt;
}

QWidget#DelegateHeaderHighlite:disabled {
    color: rgba(76,76,76,1);
    background-color: rgba(52,52,52,1);
    font-size: 7pt;
}




/* ------------------------------ SROLLBAR ----------------------------------------------*/
QAbstractScrollArea::corner {
    background: rgba(40,40,40,1);
}

/* horizontasl */
QScrollBar:horizontal {
    border: 0px;
    background: rgba(40,40,40,1);
    height: 14px;
    margin: 0px 12px 0px 12px;
}


QScrollBar::handle:horizontal {
    background: rgba(52,52,52,1);
    border: 3px solid rgba(40,40,40,1);
    min-width: 20px;
    border-radius: 7px;
}

QScrollBar::handle:horizontal:pressed {
    background: rgba(50,50,50,1);
}

QScrollBar::add-line:horizontal {
    border: 0px;
    background: rgba(52,52,52,1);
    width: 12px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
    border: 0px;
    background: rgba(52,52,52,1);
    width: 12px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar:left-arrow:horizontal{
    border: 0px;
    width: 10px;
    height: 10px;
    background: rgba(52,52,52,1);
    image: url(:/icons/widgets/dark/arrow_left.png);
}

QScrollBar:left-arrow:horizontal:pressed{
    background: rgba(50,50,50,1);
    border: 2px solid rgba(50,50,50,1);
}

QScrollBar::right-arrow:horizontal {
    border: 0px;
    width: 10px;
    height: 10px;
    background: rgba(52,52,52,1);
    image: url(:/icons/widgets/dark/arrow_right.png);
}

QScrollBar:right-arrow:horizontal:pressed{
    background: rgba(50,50,50,1);
    border: 2px solid rgba(50,50,50,1);
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

/* vertical */
QScrollBar:vertical {
    border: 0px;
    background: rgba(40,40,40,1);
    width: 14px;
    margin: 12px 0px 12px 0px;
}

QScrollBar::handle:vertical {
    background: rgba(52,52,52,1);
    border: 3px solid rgba(40,40,40,1);
    min-height: 20px;
    border-radius: 7px;
}

QScrollBar::handle:vertical:pressed {
    background: rgba(50,50,50,1);
}

QScrollBar::add-line:vertical {
    border: 0px;
    background: rgba(52,52,52,1);
    height: 12px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    border: 0px;
    background: rgba(52,52,52,1);
    height: 12px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar:up-arrow:vertical{
    border: 0px;
    height: 10px;
    width: 10px;
    background: rgba(52,52,52,1);
    image: url(:/icons/widgets/dark/arrow_up.png);
}

QScrollBar:up-arrow:vertical:pressed{
    background: rgba(50,50,50,1);
    border: 2px solid rgba(50,50,50,1);
}

QScrollBar::down-arrow:vertical {
    border: 0px;
    height: 10px;
    width: 10px;
    background: rgba(52,52,52,1);
    image: url(:/icons/widgets/dark/arrow_down.png);
}

QScrollBar:down-arrow:vertical:pressed{
    background: rgba(50,50,50,1);
    border: 2px solid rgba(50,50,50,1);
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

