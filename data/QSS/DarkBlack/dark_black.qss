/* ------------------------------- MENU -------------------------------------------------*/
QMenu {
    color: rgba(0,0,0,1);
    background-color: rgba(52,52,52,1);
    border: 1px solid rgba(208,208,208,1);
}

QMenu::item {
    color: rgba(208,208,208,1);
    background-color: transparent;
    border-left: 0px;
    border-right: 1px;
    padding: 4px 25px 4px 20px;
    height: 20px;
}

QMenu::icon {
    margin: 0px;
    padding-left:4px;
}

QMenu::separator {
    height: 1px;
    background: rgba(208,208,208,1);
    margin-left: 10px;
    margin-right: 5px;
}

QMenu::item:selected {
    color: rgba(0,0,0,1);
    background: rgba(28,131,255,0.8);  
    border-left: 0px;
    border-right: 0px;
}

/* ------------------------------- QDialog -------------------------------------------------*/

QDialog::item{
color:red;
}

/* ------------------------------- SPINBOX -------------------------------------------------*/
QSpinBox:enabled {
    background: rgba(40,40,40,1);
    color: rgba(255,255,255,1);
    border: 1px solid rgba(170,170,170,1);
    selection-background-color: rgba(28,131,255,1);  
    height: 19px;
    padding-left:5px;
}

QSpinBox:disabled {
    color: rgba(77,77,77,1);
    background: rgba(40,40,40,1);
    selection-background-color: rgba(52,52,52,1); 
    border: 1px solid rgba(170,170,170,1);
    height: 19px;
}

QSpinBox::up-button {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-radius: 2px;
    image: url(:/icons/widgets/dark/arrow_up.png);
}

QSpinBox::down-button {
    subcontrol-origin: padding;
    subcontrol-position: bottom right;
    width: 20px;
    border-radius: 2px;
    image: url(:/icons/widgets/dark/arrow_down.png);
}

/* ------------------------------- COMBOBOX -------------------------------------------------*/

QComboBox {
    padding: 0px 0px 0px 5px; /*This makes text colour work*/
}

QComboBox:enabled {
    background: rgba(40,40,40,1);
    color: rgba(255,255,255,1);
    border: 1px solid rgba(150,150,150,1);
    selection-background-color: rgba(28,131,255,1);   
    border-radius: 0px;
    height: 24px;
}


QComboBox:disabled {
    background: rgba(40,40,40,1);
    color: rgba(77,77,77,1);
    selection-background-color: rgba(52,52,52,1); 
    border 1px: solid rgba(150,150,150,1):
    border-radius: 0px;
    height: 24px;
    padding-left:5px; /*This makes text colour work*/
}


QComboBox:hover{
    border: 1px solid rgba(20,140,210,1);
    border-radius: 0px;
}

QComboBox QAbstractItemView {
    border: 1px solid rgba(39,46,56,1);
    background: rgba(40,40,40,1);
    selection-background-color: rgba(28,131,255,1);
    color: rgba(255,255,255,1);
    border-radius: 2px;
    outline:0;
}

QComboBox QAbstractItemView::disabled {
    color: rgba(208,208,208,0.6);
}


QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-radius: 2px;
    image: url(:/icons/widgets/dark/combo_arrow.png);
}

/* ------------------------------- TOOL BUTTON --------------------------------------------*/
QToolButton:enabled { /* all types of tool button */
    border: 1px solid transparent;
    background-color: transparent;
    margin-bottom:2px;
}

QToolButton:disabled {
    color: rgba(136,136,136,1);
    border: 1px solid transparent;
    background-color: transparent;
    margin-bottom:2px;
}

QToolButton:hover {
    color: rgba(255,255,255,1);
    border: 1px solid rgba(39,151,255,1);
    margin-bottom:2px;
    background: rgba(28,131,255,0.5);  
}

QToolButton[popupMode="1"] { /* only for MenuButtonPopup */
    padding-right: 15px; /* make way for the popup button */
}

QToolButton:pressed {
    color: rgba(255,255,255,1);
    border: 1px solid rgba(1,96,191,1);
    background-color: rgba(52,52,52,1);
}

QToolButton:checked{
    border: 1px solid transparent;
    background-color: rgba(52,52,52,1);
    border-right:1px solid rgba(20,20,20,1);
    border-bottom:1px solid rgba(20,20,20,1);
}

QToolButton#TreeToolButton:checked{
    background-color:transparent;
    border:none;
}

/* the subcontrols below are used only in the MenuButtonPopup mode */
QToolButton::menu-button {
    border: 0px;
    padding-right:10px;
}

