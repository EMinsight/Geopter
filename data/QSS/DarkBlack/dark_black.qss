/* default background */
QMainWindow {
    color: rgba(0,0,0,1);
    background: rgba(52,52,52,1);
    border-color: black;
}

QMainWindow::separator {
    color: rgba(30,30,30,1);
    background: rgba(52,52,52,1); /* Rand zum 3D View hin */
    border: 0px solid rgba(208,208,208,1);
}

QWidget {
    color: rgba(208,208,208,1);
    background: rgba(30,30,30,1); /*Hintergrund und der dr√ºnne Strich beim Pane*/
    border: 0px solid transparent;
    font-size: 9pt;
}


QGroupBox{
    border: 1px solid rgba(136,136,136,1);
    padding:25px 10px 10px 10px;
}

QGroupBox::title{
    color:rgba(176,210,241,1);
    top:5px;
    left:5px;
}

QListWidget{
    border: 1px solid rgba(136,136,136,1);
    outline:0;
}

QListWidget::item {
    background:transparent;
}

QListWidget::item:hover {
    color: rgba(28,131,255,1);
    background:transparent;
}

QListWidget::item:selected {
    color: rgba(28,131,255,1);
    background:transparent;
}

QFrame#SepLine {
    color: red;
    background: transparent;
}

/* ------------------------------- QUICK ACESS TOOL BAR --------------------------------------------*/
QWidget#QuickAcess {
    color: rgba(0,0,0,1);    
    background: rgba(40,40,40,1);  
    border-bottom: 1px solid rgba(150,150,150,1);
}

QWidget#QuickAcess:pressed {
    color: rgba(255,255,255,1);
    border: 1px solid rgba(1,96,191,1);
    background-color: rgba(52,52,52,1);
}

QFrame#QuickAcessSep {
    background: none; /* Trennstrich bei Open, Save usw. */
    border: 0px;
    width: 1px;
    margin-top:4px;
    margin-bottom:4px;
}

/* ------------------------------- RIBBON TOOL BAR --------------------------------------------*/
QToolBar {
    background: rgba(30,30,30,1);
    margin: 0px;
    border: 0px; 
    spacing: 10;
}

QWidget#RibbonLabel {
    background: rgba(30,30,30,1);
    border: 0px;
}

QWidget#Ribbon { /* The ribbon tab widget content */
    color: #F0F0F0;
    background: rgba(40,40,40,1);
}

QTabBar#ribbonTabBar {
    background: rgba(30,30,30,1);
    border-bottom: 1px solid rgba(150,150,150,1);
}

QTabWidget::pane {
  border-top: 1px solid rgba(150,150,150,1);
  top:-1px; 
} 

QTabWidget#ribbonTabWidget::tab-bar {
    left: 15px; /* move to the right */
}

QTabBar#ribbonTabBar::tab:selected {
    color: rgba(255,255,255,1);
    border-left: 1px solid rgba(150,150,150,1);
    border-right: 1px solid rgba(150,150,150,1);
    border-top: 1px solid rgba(150,150,150,1);
    border-top-left-radius:2px;
    border-top-right-radius:2px;
    background: rgba(40,40,40,1);  
    margin-top: 4px; /*so the quickaceesbar has soem space left */
    margin-left: 4px;
    padding-top: 3 px;
    padding-bottom: 5 px;
    padding-left: 10 px;
    padding-right: 10 px;
    margin-bottom:-1px;
}

QTabBar#ribbonTabBar::tab:!selected:hover {
    color: rgba(255,255,255,1);
    border-left: 1px solid rgba(150,150,150,1);
    border-right: 1px solid rgba(150,150,150,1);
    border-top: 1px solid rgba(150,150,150,1);
    border-bottom: 1px solid rgba(150,150,150,1);
    border-top-left-radius:2px;
    border-top-right-radius:2px;
    background: rgba(30,30,30,1);
    margin-top: 4px; /*so the quickaceesbar has soem space left */
    margin-left: 4px;
    padding-top: 3 px;
    padding-bottom: 3 px;
    padding-left: 10 px;
    padding-right: 10 px;
}

QTabBar#ribbonTabBar::tab:!selected {    
    color: rgba(208,208,208,1);
    border-top:1px solid transparent;
    border-left:1px solid transparent;
    border-right:1px solid transparent;
    border-bottom: 1px solid rgba(150,150,150,1);
    border-top-left-radius:2px;
    border-top-right-radius:2px;
    background: rgba(30,30,30,1);
    margin-top: 4px; /*so the quickaceesbar has soem space left */
    margin-left: 4px;
    padding-top: 3 px;
    padding-bottom: 3 px;
    padding-left: 10 px;
    padding-right: 10 px;
} 

QTabBar#ribbonTabBar::tab:!enabled:!selected {
    color: rgba(208,208,208,0.6);
    margin-top: 4px; /*so the quickaceesbar has soem space left */
    margin-left: 4px;
    padding-top: 3 px;
    padding-bottom: 3 px;
    padding-left: 10 px;
    padding-right: 10 px;
}


